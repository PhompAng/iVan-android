<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
			 xmlns:app="http://schemas.android.com/apk/res-auto">
	<font android:font="@font/kanit_light"
		android:fontStyle="normal"
		android:fontWeight="300"
		app:font="@font/kanit_light"
		app:fontStyle="normal"
		app:fontWeight="300"/>
	<font android:font="@font/kanit_regular"
		  android:fontStyle="normal"
		  android:fontWeight="400"
		  app:font="@font/kanit_regular"
		  app:fontStyle="normal"
		  app:fontWeight="400"/>
	<font android:font="@font/kanit_medium"
		  android:fontStyle="normal"
		  android:fontWeight="500"
		  app:font="@font/kanit_medium"
		  app:fontStyle="normal"
		  app:fontWeight="500"/>
	<font android:font="@font/kanit_medium_italic"
		  android:fontStyle="italic"
		  android:fontWeight="500"
		  app:font="@font/kanit_medium_italic"
		  app:fontStyle="italic"
		  app:fontWeight="500"/>
	<font android:font="@font/kanit_bold"
		  android:fontStyle="normal"
		  android:fontWeight="700"
		  app:font="@font/kanit_bold"
		  app:fontStyle="normal"
		  app:fontWeight="700"/>
</font-family>
